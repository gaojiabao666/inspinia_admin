<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xsqwe.admin.mapper.StudentMapper">
    <insert id="insertStu" parameterType="com.xsqwe.admin.pojo.Student" useGeneratedKeys="true" keyProperty="stuId">
        insert into student(`stu_name`,`address`) value(#{stuName},#{address})
    </insert>

</mapper>